{
  "name": "@becomes/cms-sdk",
  "version": "3.1.1",
  "description": "SDK for client side of BCMS",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "dev": "webpack serve --host 0.0.0.0 --port 1281",
    "build:ts": "tsc -p .",
    "build": "npm-tool --build",
    "bundle": "npm-tool --bundle",
    "publish": "npm-tool --publish",
    "link": "npm-tool --link",
    "link:sudo": "npm-tool --link --sudo",
    "unlink": "npm-tool --unlink",
    "unlink:sudo": "npm-tool --unlink --sudo",
    "pack": "npm-tool --pack",
    "typedoc-generate": "typedoc",
    "test": "mocha -r ts-node/register test/integration/*.test.ts --recursive --exit",
    "test:active": "mocha -r ts-node/register test/integration/*.active.test.ts --recursive --exit",
    "test:unit": "mocha -r ts-node/register test/unit/*.test.ts --recursive --exit",
    "test:unit:active": "mocha -r ts-node/register test/unit/*.active.test.ts --recursive --exit",
    "test:coverage": "npm-tool --test-coverage",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "socket.io-client": "^4.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@banez/npm-tool": "^1.0.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.5",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "chai": "^4.3.4",
    "env-cmd": "^10.0.1",
    "eslint": "^7.30.0",
    "form-data": "^4.0.0",
    "fs-extra": "^9.1.0",
    "mocha": "^9.1.3",
    "prettier": "^2.3.2",
    "ts-node": "^8.6.1",
    "typescript": "^4.3.5",
    "yamljs": "^0.3.0"
  }
}
